					name3 :=0;
				end;
				name3 :=0;
			end;
			name3 :=0;
		end;
		name3 := 0;
		
		begin
			var name4 : int :=0;
			name4 :=0;
			begin
				var name3 : int :=0;
				begin
					var name4 : int :=0;
					name4 :=0;
				end;
				name3 :=0;
			end;
		end;
		begin
			var name4 : int :=0;
			begin
				var name3 : int :=0;
				begin
					var name4 : int :=0;
					begin
						var name3 : int :=0;
						name6 :=0;
					end;
					name4 :=0;
					begin
						var name3 : int :=0;
						begin
							var name4 : int :=0;
							begin
								var name3 : int :=0;
								name6 :=0;
							end;
							name4 :=0;
						end;
						name3 :=0;
					end;
					name4 :=0;
				end;
				name3 :=0;
			end;
			name4 :=0;
		end;
		return 5;
	end;
end;



